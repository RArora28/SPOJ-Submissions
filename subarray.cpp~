#include<iostream>
#include<limits.h>
#include<cstdio>
using namespace std;

void create(int a[], int tree[], int low, int high, int pos)
{
  if(low == high)
    {
      tree[pos] = a[low];
      return;
    }

  int mid = (low + high) / 2;

  tree[2 * pos + 1] = create(a, tree, low, mid, 2 * pos + 1);
  tree[2 * pos + 2] = create(a, tree, mid + 1, high, 2 * pos + 2);
  tree[pos] = min(tree[2 * pos + 2], tree[2 * pos + 1]);
  return;
}

int rmq(int tree[], int low, int high, int qlow, int qhigh, int pos)
{
  if(low <= qlow && high >= qhigh)
    return tree[pos];
  else if(qhigh < low || qlow > high)
    return INT_MAX;
  else
    {
      int mid = (low + high) / 2;
      return min(rmq(tree, low, mid, qlow, qhigh, 2*pos + 1), rmq(tree, mid + 1,high, qlow, qhigh, 2*pos + 2));
    }
}

int main()
{
  int n, k, a[1000010], tree[4000040];
  
  cin>>n>>k;

  for(int i = 0; i < n; i++)
    cin>>a[i];

  memset(tree, INT_MAX, 4*n);
  
  create(a, tree, 0, n-1, 0);
    
  for(i = 0; i + k < n; i++)
    cout<<rmq(tree, 0, n-1, i, i + k, 0)<<endl;
  
  return 0;
}
